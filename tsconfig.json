{
  "compilerOptions": {
    "noPropertyAccessFromIndexSignature": true,
    "target": "ES2022",
    "lib": ["ES2022", "dom"],
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "@_e2e/*": ["e2e/*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "e2e/output/**"]
}
